<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:background="#FFFFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/rra951icwl4r"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FFF9F9F9"
        android:elevation="6dp"
        android:fillViewport="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/r1362w6rgid5"
                android:layout_width="311dp"
                android:layout_height="239dp"
                android:gravity="center"
                android:layout_marginHorizontal="5dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/rgwvljvteos7"
                    android:layout_width="240dp"
                    android:layout_height="225dp"
                    android:layout_marginTop="6dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/pnlibrary" />
            </LinearLayout>
            <TextView
                android:id="@+id/r2zek7ayt8qh"
                android:textColor="#df2ee8"
                android:textSize="32sp"
                android:textStyle="bold"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="Xin chào! "
                />
            <LinearLayout
                android:id="@+id/r0upywpvxxs6q"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingVertical="7dp"
                android:layout_marginBottom="17dp"
                android:layout_marginHorizontal="24dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r8o87zzr2rzc"
                    android:textColor="#424955"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_marginBottom="4dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Tên đăng nhập"
                    />
                <LinearLayout
                    android:id="@+id/ra5xncbzm1rr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:background="@drawable/cr4bfff3f4f6"
                    android:paddingVertical="15dp"
                    android:paddingHorizontal="19dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/rdjscxr78zro"
                        android:layout_width="12dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="14dp"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/ry8h6btmuif"
                            android:layout_width="match_parent"
                            android:layout_height="7dp"
                            android:layout_marginBottom="3dp"
                            android:layout_marginHorizontal="2dp"
                            android:src="@mipmap/ic_launcher"
                            android:scaleType="fitXY"
                            />
                        <ImageView
                            android:id="@+id/rwbboxhnvexd"
                            android:layout_width="match_parent"
                            android:layout_height="5dp"
                            android:src="@mipmap/ic_launcher"
                            android:scaleType="fitXY"
                            />
                    </LinearLayout>

                    <EditText
                        android:id="@+id/editTextText2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:inputType="text"
                        android:text="Name" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r347s12pm2w6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingVertical="6dp"
                android:layout_marginBottom="50dp"
                android:layout_marginHorizontal="27dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rszwfbzvu6fl"
                    android:textColor="#424955"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_marginBottom="5dp"
                    android:layout_marginStart="1dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Mật khẩu"
                    />
                <LinearLayout
                    android:id="@+id/rj9xo0ahi1vm"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:background="@drawable/cr4bfff3f4f6"
                    android:paddingVertical="15dp"
                    android:paddingHorizontal="19dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/r5migeoqe984"
                        android:layout_width="12dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="14dp"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/r726pzzmwluo"
                            android:layout_width="match_parent"
                            android:layout_height="4dp"
                            android:layout_marginBottom="3dp"
                            android:layout_marginHorizontal="2dp"
                            android:src="@mipmap/ic_launcher"
                            android:scaleType="fitXY"
                            />
                        <RelativeLayout
                            android:id="@+id/rgk605qe9p3u_rl"
                            android:layout_width="match_parent"
                            android:layout_height="8dp"
                            >
                            <ImageView
                                android:id="@+id/rgk605qe9p3u"
                                android:layout_width="match_parent"
                                android:layout_height="8dp"
                                android:src="@mipmap/ic_launcher"
                                android:scaleType="fitXY"
                                />
                            <LinearLayout
                                android:paddingHorizontal="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:id="@+id/rh9u6bqyt4do"
                                    android:layout_width="match_parent"
                                    android:layout_height="2dp"
                                    android:background="@drawable/sff171a1fsw1"
                                    android:layout_marginTop="3dp"
                                    android:orientation="vertical">
                                </LinearLayout>
                            </LinearLayout>
                        </RelativeLayout>
                    </LinearLayout>

                    <EditText
                        android:id="@+id/editTextTextPassword"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:hint="NHẬP MẬT KHẨU"
                        android:inputType="textPassword" />

                    <LinearLayout
                        android:id="@+id/rmtu6ojscdp"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/rq04eevoxbzm"
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/rbfgvb7jd1qu"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">
                            <RelativeLayout
                                android:id="@+id/rumqizp22efe_rl"
                                android:layout_width="match_parent"
                                android:layout_height="14dp"
                                >
                                <ImageView
                                    android:id="@+id/rumqizp22efe"
                                    android:layout_width="match_parent"
                                    android:layout_height="14dp"
                                    android:src="@mipmap/ic_launcher"
                                    android:scaleType="fitXY"
                                    />
                            </RelativeLayout>
                            <ImageView
                                android:id="@+id/rnixv325azrm"
                                android:layout_width="match_parent"
                                android:layout_height="11dp"
                                android:src="@mipmap/ic_launcher"
                                android:scaleType="fitXY"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <Button
                android:id="@+id/button3"
                android:layout_width="356dp"
                android:layout_height="wrap_content"
                android:background="@drawable/custom_buttom"
                android:text="ĐĂNG NHẬP"
                android:textSize="20dp"
                android:layout_marginBottom="10dp"
                app:backgroundTint="@null" />

            <LinearLayout
                android:id="@+id/rd35imvw5e6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="12dp"
                android:layout_marginHorizontal="86dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/r2ncv6w8bp9n"
                    android:layout_width="74dp"
                    android:layout_height="1dp"
                    android:orientation="vertical">
                </LinearLayout>
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <TextView
                    android:id="@+id/rkmxz226bddm"
                    android:textColor="#9095a0"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Hoặc"
                    />
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <LinearLayout
                    android:id="@+id/rdd316snm56c"
                    android:layout_width="68dp"
                    android:layout_height="1dp"
                    android:orientation="vertical">
                </LinearLayout>
            </LinearLayout>

            <Button
                android:id="@+id/button5"
                android:layout_width="356dp"
                android:layout_height="wrap_content"
                android:background="@drawable/custom2"
                android:text="ĐĂNG KÝP"
                android:textColor="#FFE35CE8"
                android:textSize="20dp"
                app:backgroundTint="@null" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>

